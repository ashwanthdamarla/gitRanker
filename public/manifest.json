{
  "name": "Git Ranker",
  "version": "1.0",
  "manifest_version": 2,
  "browser_action": {
      "default_popup": "index.html",
      "default_title": "Open the extension"
  },
  "permissions": [
    "tabs",
    "notifications",
    "http://*/",
    "https://*/"
  ],
  "background": {
    "scripts": ["background.js"]
  },
  "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*"],
      "js": ["contentScript.js"]
    }
  ],
  "web_accessible_resources": [
    "index.html"
  ]
}
